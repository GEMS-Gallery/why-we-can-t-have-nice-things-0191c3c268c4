type Time = int;
type Result = 
 variant {
   err: text;
   ok: BlogPost;
 };
type Image = 
 record {
   description: opt text;
   id: nat;
   url: text;
 };
type BlogPost = 
 record {
   content: text;
   id: nat;
   timestamp: Time;
   title: text;
 };
service : {
  addGalleryImage: (text, opt text) -> (nat);
  createBlogPost: (text, text) -> (nat);
  getBlogPost: (nat) -> (Result) query;
  getBlogPosts: () -> (vec BlogPost) query;
  getGalleryImages: () -> (vec Image) query;
}
